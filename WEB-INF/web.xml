<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee" 
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">

	<servlet>
        <servlet-name>OAuth Callback Servlet</servlet-name>
        <servlet-class>com.idega.block.login.servlet.OAuth2CallbackServlet</servlet-class>
        <load-on-startup>210</load-on-startup>
    </servlet>
    
    <servlet>
        <servlet-name>Facebook channel Servlet</servlet-name>
        <servlet-class>com.idega.block.login.servlet.FacebookChannelServlet</servlet-class>
        <load-on-startup>211</load-on-startup>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>OAuth Callback Servlet</servlet-name>
        <url-pattern>/oauth2callback</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Facebook channel Servlet</servlet-name>
        <url-pattern>/channel</url-pattern>
    </servlet-mapping>
</web-app>